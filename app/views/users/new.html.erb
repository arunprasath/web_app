<%= link_to "Back to User List", users_path %>
<br /><br />
<fieldset>
<legend>New User</legend>
<% form_for @user, :url => { :action => "create" }  do |f| %>
  <%#= error_messages_for :user, :header_message => "Please Try Again!", :message => "We had some problems with following fields:" %>
  <%= render :partial => "form", :object => f %><br />
  <%= f.submit "Register" %>
<% end %>
</fieldset>

