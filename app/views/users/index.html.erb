<h1>List of Users</h1>

<%= link_to "Create New User", new_user_path %><br /><br />
<% @users.each do |user| %>
  <b><%= user.login %></b><br />
  <%= user.address %><br />
  <%= user.city %><br />
  <%= link_to "Edit", edit_user_path(user) %> | <%= link_to "Destroy", :method => :delete %><br /><br />

<% end %>

